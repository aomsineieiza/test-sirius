<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="android:inputType" />
        <attr name="android:padding" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="CustomEditText_setBackgroundColor" format="color" />
        <attr name="CustomEditText_setBorderColor" format="color" />
        <attr name="CustomEditText_setCornerRadius" format="dimension" />
        <attr name="CustomEditText_setClearIconVisible" format="boolean" />
        <attr name="CustomEditText_clearIconTint" format="color" />
        <attr name="CustomEditText_setSearchIconVisible" format="boolean" />
        <attr name="CustomEditText_searchIconTint" format="color" />
        <attr name="CustomEditText_setCursorColor" format="color" />
        <attr name="CustomEditText_setPrefix" format="string" />
        <attr name="CustomEditText_setPrefixTextColor" format="color" />
        <attr name="CustomEditText_setElevation" format="float" />
        <attr name="CustomEditText_inputType" format="enum">
            <enum name="creditCardNo" value="1" />
            <enum name="expireDate" value="2" />
        </attr>
    </declare-styleable>
</resources>